{
    "manifest_version": 3,
    "name": "Form Autofill Assistant",
    "version": "1.0",
    "description": "Fill out pet client forms quickly!",
    "permissions": ["activeTab", "scripting"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "host_permissions": ["<all_urls>"]
}
